<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "edu.unlam.asistente.database.pojo.Usuario" table = "USUARIOS">
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <set name = "eventos" cascade="save-update" table="usuarioEventos">
         <key column = "id_usuario"/>
         <many-to-many column = "id_evento" class="Evento"/>
      </set>
      
      <property name = "usuario" column = "usuario" type = "string"/>
   </class>
</hibernate-mapping>